{
  "commands": {
    "crypto-validate": {
      "description": "Validate crypto implementation",
      "parameters": {
        "algorithm": "Algorithm to validate"
      },
      "example": "/crypto-validate ml-kem",
      "output": "Validation results"
    },
    "network-simulate": {
      "description": "Run network simulation",
      "parameters": {
        "scenario": "Simulation scenario"
      },
      "example": "/network-simulate partition",
      "output": "Simulation results"
    },
    "dag-visualize": {
      "description": "Visualize DAG state",
      "parameters": {
        "state": "State to visualize"
      },
      "example": "/dag-visualize current",
      "output": "DAG visualization"
    },
    "fuzz-test": {
      "description": "Execute fuzz testing",
      "parameters": {
        "target": "Target to fuzz"
      },
      "example": "/fuzz-test crypto-ml-kem",
      "output": "Fuzzing results"
    }
  }
}